source ~/.vimperatorrc.sensitive

set focuscontent
set hlsearch

set guioptions=bn
set complete+=t
set urlseparator=''
set showstatuslinks=2
set newtab=all

"kills ff's ^n shortcut
cmap <C-n> <Nop>
imap <C-n> <Nop>

"readability default shortcut
map <Leader>r <C-Z> <C-A-R> <ESC>
"torbutton
map <Leader>t <C-Z><C-2><ESC>

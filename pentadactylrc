source ~/.pentadactylrc.sensitive
set hlfind

set guioptions=bns
set complete=slSf
set urlseparator=''
set showstatuslinks=command
set newtab=all

"kill ff's ^n and ^o shortcuts
cmap <C-n> <Nop>
imap <C-n> <Nop>
imap <C-o> <Nop>

"enjoy reading shortcut
map <Leader>r :javascript enjoyreading.onToolbarButtonCommand()<CR>
"torbutton
map <Leader>t <C-Z><C-2><Esc>


set! browser.sessionstore.max_concurrent_tabs=0

hi StatusLineNormal color: black !important; background: #a0a0a0 !important;
